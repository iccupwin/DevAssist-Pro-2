# API Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_USE_REAL_API=true

# WebSocket Configuration
REACT_APP_WS_URL=ws://localhost:8000

# AI API Keys (Required for AI functionality)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_api_key_here
REACT_APP_GOOGLE_API_KEY=your_google_ai_api_key_here

# AI Models Configuration (Optional for frontend display)
REACT_APP_DEFAULT_ANALYSIS_MODEL=claude-3-5-sonnet-20240620
REACT_APP_DEFAULT_COMPARISON_MODEL=gpt-4o

# Environment
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_STREAMING=true
REACT_APP_ENABLE_ANALYTICS=false

# UI Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_FORMATS=pdf,docx,doc,txt