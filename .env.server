# Server Environment Configuration
# Скопируйте этот файл в .env и заполните настройки

# Server Configuration
SERVER_IP=YOUR_SERVER_IP_HERE

# Database Configuration
POSTGRES_PASSWORD=secure_postgres_password_123
REDIS_PASSWORD=redis_secure_password_123

# JWT Configuration
JWT_SECRET=your_very_secure_jwt_secret_key_minimum_32_characters_long

# AI Provider API Keys (ОБЯЗАТЕЛЬНО!)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Additional AI providers
CLAUDE_API_KEY=your_claude_api_key_here

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost